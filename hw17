            let baatir = 33;
            
            
            if (baatir >= 1 && baatir <= 20) {
                console.log("First floor");
            } else if (baatir >= 21 && baatir <= 48) {
                console.log("Second floor");
            } else if ((baatir >= 49 && baatir <= 90)) {
                console.log("Third floor");
